$(document).ready(function(){

    $("[rel='tooltip']").tooltip();

    $('#hover-cap .thumbnail').hover(
        function(){
            $(this).find('.caption').fadeIn(500); //.fadeIn(250)
        },
        function(){
            $(this).find('.caption').fadeOut(500); //.fadeOut(205)
        }
    );

});
</script>

